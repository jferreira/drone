<h1>Bookings#index</h1>
<p>Find me in app/views/bookings/index.html.erb</p>


<div class="bookings-container">
  <% @bookings.each do |booking| %>
    <p class="booking-title">Title: <%= booking.title %></p>
    <p class="booking-location">Location: <%= booking.location %></p>
    <p class="booking-location">Status: <%= booking.status %></p>
    <%= link_to 'Edit Booking', edit_package_booking_path(params[:package_id], booking) %>
  <% end %>
</div>


